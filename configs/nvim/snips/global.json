{
  "log": {
    "prefix": "log",
    "body": [
      "[${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}]: ${1:log_message}"
    ],
    "description": "set the date and time and start loggin"
  },
  "entry": {
    "prefix": "entry",
    "body": [
      "--------------------------------------------------------------------------------",
      "${CURRENT_DAY_NAME_SHORT} ${CURRENT_MONTH_NAME_SHORT} ${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND} ${CURRENT_YEAR}",
      "${1:entry}"
    ],
    "description": "a different format to set date and time for log entries"
  }
}
